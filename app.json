{
  "expo": {
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "fonts": {
        "Neue Haas Grotesk": "https://fonts.googleapis.com/css2?family=Neue+Haas+Grotesk:wght@400;700&display=swap",
        "Merriweather": "https://fonts.googleapis.com/css2?family=Merriweather:wght@400;700&display=swap"
      },
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "name": "Hidden",
    "slug": "Hidden",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hidden",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "jsEngine": "jsc",
    "experiments": {
      "typedRoutes": true
    }
  }
}
